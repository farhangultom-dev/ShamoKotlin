<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.farhandev.shamokotlin.ui.home.fragment.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.farhandev.shamokotlin.ui.home.fragment.message.MessageFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_wishlist"
        android:name="com.farhandev.shamokotlin.ui.home.fragment.wishlist.WishlistFragment"
        android:label="@string/wishlist"
        tools:layout="@layout/fragment_wishlist"/>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.farhandev.shamokotlin.ui.home.fragment.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />
</navigation>